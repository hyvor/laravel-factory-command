{
    "name": "hyvor/laravel-factory-command",
    "description": "Call Laravel Model Factories via a command",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hyvor\\LaravelFactoryCommand\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hyvor\\LaravelFactoryCommand\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Supun",
            "email": "supun@hyvor.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.3",
        "pestphp/pest": "^2.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hyvor\\LaravelFactoryCommand\\FactoryCommandServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
